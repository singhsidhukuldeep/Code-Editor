{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, { "ignore": ["electron"] }],
    "linebreak-style": 0,
    "no-underscore-dangle": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
